<Window x:Class="DAN_LI.View.Patient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DAN_LI.View"
        mc:Ignorable="d"
        Title="Patient"
        Height="450"
        Width="800">
    <Grid>
        <Button Content="Sick Leave Requirement"
                Command="{Binding requirement, UpdateSourceTrigger=PropertyChanged}"
                HorizontalAlignment="Left"
                Margin="80,60,0,0"
                VerticalAlignment="Top"
                Width="155" />
        <Button Content="Log Out"
                Command="{Binding logOut, UpdateSourceTrigger=PropertyChanged}"
                HorizontalAlignment="Left"
                Height="20"
                Margin="590,60,0,0"
                VerticalAlignment="Top"
                Width="155" />
    </Grid>
</Window>
